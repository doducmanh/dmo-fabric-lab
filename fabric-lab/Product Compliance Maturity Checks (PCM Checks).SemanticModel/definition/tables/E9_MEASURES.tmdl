	measure RATING_NUMBER =
			
			VAR DeltaF = [DELTA_F]
			VAR DeltaN = [DELTA_N]
			VAR DeltaP = [DELTA_P]
			VAR DeltaL = [DELTA_L]
			
			VAR WeightedSum =
			    IF ( DeltaN < 0, 0, DeltaN )
			        + IF ( DeltaP < 0, 0, DeltaP * 2 )
			        + IF ( DeltaL < 0, 0, DeltaL * 3 )
			        + IF ( DeltaF < 0, 0, DeltaF * 4 )
			VAR TotalSum =
			    IF ( DeltaN < 0, 0, DeltaN )
			        + IF ( DeltaP < 0, 0, DeltaP )
			        + IF ( DeltaL < 0, 0, DeltaL )
			        + IF ( DeltaF < 0, 0, DeltaF )
			VAR result =
			    IF ([COUNT_RATING]=0,
			        BLANK(),
			        ROUND ( IFERROR ( WeightedSum / TotalSum, 4 ), 0 )
			    )
			RETURN
			    result
