	measure PRESET_FILTER_PCM_CHECK_YEAR =
			
			VAR FILTER_PCM_YEAR_TABLE =
			    FILTER (
			        ALLSELECTED ( E9_PCM_CHECK ),
			        E9_PCM_CHECK[PCM_CHECK_YEAR]
			            = SELECTEDVALUE ( E9_DISTINCT_PCM_CHECK_YEAR[PCM_CHECK_YEAR]) - MAX ( 'E9_PARA_PREVIOUS_YEAR'[Para_Previous_Year] )
			    )
			VAR START_POINT =
			    DATE ( YEAR (
			        CALCULATE (
			            [MIN_DATE_PCM_CHECK],
			            ALLSELECTED ( E9_DIM_DATE ),
			            FILTER_PCM_YEAR_TABLE
			        )
			    ), 01, 01)
			VAR MAX_PCM_CHECK_YEAR =
			    YEAR (
			        CALCULATE (
			            [MAX_DATE_PCM_CHECK],
			            ALLSELECTED ( E9_DIM_DATE ),
			            FILTER_PCM_YEAR_TABLE
			        )
			    )
			VAR PCM_CHECK_YEAR =
			    CALCULATE (
			        MAX ( E9_PCM_CHECK[PCM_CHECK_YEAR] ),
			        ALLSELECTED ( E9_DIM_DATE ),
			        FILTER_PCM_YEAR_TABLE
			    )
			VAR END_POINT =
			    IF (
			        MAX_PCM_CHECK_YEAR > YEAR ( START_POINT ),
			        CALCULATE (
			            [MAX_DATE_PCM_CHECK],
			            ALLSELECTED ( E9_DIM_DATE ),
			            FILTER_PCM_YEAR_TABLE
			        ),
			            IF(
			                YEAR(TODAY()) <= YEAR ( START_POINT ) && MONTH(TODAY())>= 10,
			                EOMONTH(DATE ( YEAR ( START_POINT ), 12, 31 ),3),
			                DATE ( YEAR ( START_POINT ), 12, 31 )
			            )
			    )
			RETURN SWITCH (
			        TRUE (),
			        AND (
			            START_POINT <= MAX ( E9_DIM_DATE[FullDateAlternateKey] ),
			            MIN ( E9_DIM_DATE[FullDateAlternateKey] ) <= END_POINT
			        ), 1,
			        0
			    )
		formatString: 0
		displayFolder: OVER_TIME
		lineageTag: f44fde64-6075-4875-b019-b4ab83c12bbe

