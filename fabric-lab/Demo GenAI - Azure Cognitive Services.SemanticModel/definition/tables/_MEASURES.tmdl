	measure 'Preset Value' =
			
			VAR presetValue =
			    SELECTEDVALUE ( 'OVERDUE_RANGE'[Overdue Range] )
			VAR result =
			    SWITCH (
			        presetValue,
			        "All", IF ( SUM ( X_IFR_EPIC6_TRAINING_RATE[ASS_OVERDUE_PCT] ) >= 0, 1, 0 ),
			        "Overdue >=2%", IF ( SUM ( X_IFR_EPIC6_TRAINING_RATE[ASS_OVERDUE_PCT] ) >= 0.02, 1, 0 ),
			        "Overdue <2%", IF ( SUM ( X_IFR_EPIC6_TRAINING_RATE[ASS_OVERDUE_PCT] ) < 0.02, 1, 0 )
			    )
			RETURN
			    result
		formatString: 0
