	measure 'Preset Value UNPVT Pie-expand' =
			
			VAR presetValue =
			    SELECTEDVALUE ( 'OVERDUE_RANGE'[Overdue Range] )
			VAR result =
			    SWITCH (
			        presetValue,
			        "All",
			            IF (
			                CALCULATE (
			                    _MEASURES[%_ASGMTS],
			                    X_IFR_EPIC6_TRAINING_RATE_UNPIVOT[ASS_STATUS] = "Overdue"
			                ) >= 0.00,
			                1,
			                0
			            ),
			        "Overdue >=2%",
			            IF (
			                CALCULATE (
			                    _MEASURES[%_ASGMTS],
			                    X_IFR_EPIC6_TRAINING_RATE_UNPIVOT[ASS_STATUS] = "Overdue"
			                ) >= 0.02,
			                1,
			                0
			            ),
			        "Overdue <2%",
			            IF (
			                CALCULATE (
			                    _MEASURES[%_ASGMTS],
			                    X_IFR_EPIC6_TRAINING_RATE_UNPIVOT[ASS_STATUS] = "Overdue"
			                ) < 0.02,
			                1,
			                0
			            )
			    )
			RETURN
			    result
		formatString: 0
		displayFolder: UNPIVOT_TABLE
		lineageTag: 1e957410-1957-47d7-9810-6e89ef72f576

