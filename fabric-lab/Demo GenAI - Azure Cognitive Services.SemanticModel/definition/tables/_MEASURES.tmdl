	measure 'Preset Value UNPVT' =
			
			VAR presetValue =
			    SELECTEDVALUE ( 'OVERDUE_RANGE'[Overdue Range] )
			VAR result =
			    SWITCH (
			        presetValue,
			        "All", IF ( _MEASURES[%_ASGMTS] >= 0, 1, 0 ),
			        "Overdue >=2%", IF ( _MEASURES[%_ASGMTS] >= 0.02, 1, 0 ),
			        "Overdue <2%", IF ( _MEASURES[%_ASGMTS] < 0.02, 1, 0 )
