	measure Rank =
			IF(
			    SELECTEDVALUE(Sort[Name]) = "ASC",
			    SWITCH(
			        SELECTEDVALUE('Sort By'[Measure]),
			        "Territory", [Territory Rank],
			        "Sale", RANKX(ALLSELECTED(Regions[Territory]),[Total Sales],,ASC),
			        RANKX(ALLSELECTED(Regions[Territory]),[Total Sales],,ASC)
			    ),
			    SWITCH(
			        SELECTEDVALUE('Sort By'[Measure]),
			        "Territory", [Territory Rank DESC],
			        "Sale", RANKX(ALLSELECTED(Regions[Territory]),[Total Sales],,DESC),
			        RANKX(ALLSELECTED(Regions[Territory]),[Total Sales],,DESC)
			    )
			)
		formatString: 0
		lineageTag: 9f2b7379-f0cc-4a4b-a3ca-c7989f3fbc64

	measure 'Territory Rank' = ```
			
			VAR CurrentTer = SELECTEDVALUE(Regions[Territory])
			VAR Result = 
			    COUNTROWS(
			        FILTER(
			            ALL(Regions[Territory]),
			            Regions[Territory] <= CurrentTer
			        )
			    )
			RETURN Result
			```
		formatString: 0
		lineageTag: aed15087-248e-496d-b4ea-5e955996cf40

	measure 'Territory Rank DESC' = ```
			
			VAR CurrentTer = SELECTEDVALUE(Regions[Territory])
			VAR Result = 
			    COUNTROWS(
			        FILTER(
			            ALL(Regions[Territory]),
			            Regions[Territory] > CurrentTer
			        )
			    )
			RETURN Result
			```
		formatString: 0
