	measure XS_RAW_NO_ROUND =
			VAR ISNA =
			    IF ( MAX ( E10_PC_CHECKS_L2_CATEGORIES[RATING] ) IN { "NA" }, 0, BLANK () )
			VAR RESULT =
			    IF (
			        ISBLANK ( [DELTA_F] ) || ISBLANK ( [DELTA_L] )
			            || ISBLANK ( [DELTA_P] )
			            || ISBLANK ( [DELTA_N] ),
			        ISNA,
			        IFERROR (
			            (
			                IF ( [DELTA_N] < 0, 0, [DELTA_N] )
			                    + IF ( [DELTA_P] < 0, 0, [DELTA_P] * 2 )
			                    + IF ( [DELTA_L] < 0, 0, [DELTA_L] * 3 )
			                    + IF ( [DELTA_F] < 0, 0, [DELTA_F] * 4 )
			            )
			                / (
			                    IF ( [DELTA_N] < 0, 0, [DELTA_N] )
			                        + IF ( [DELTA_P] < 0, 0, [DELTA_P] )
			                        + IF ( [DELTA_L] < 0, 0, [DELTA_L] )
			                        + IF ( [DELTA_F] < 0, 0, [DELTA_F] )
			                ),
			            4
			        )
			    )
			RETURN
			    RESULT
		displayFolder: RISK_CATEGORIES
		lineageTag: e90f3823-1f71-440f-9c00-578285185353

		annotation PBI_FormatHint = {"isGeneralNumber":true}

