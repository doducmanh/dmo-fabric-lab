	measure PRESET_FILTER_PC_CHECK_YEAR =
			VAR FILTER_PC_YEAR_TABLE =
			    FILTER (
			        ALLSELECTED ( E10_PC_CHECKS ),
			        E10_PC_CHECKS[PC_CHECK_YEAR]
			            = SELECTEDVALUE ( E10_DISTINCT_PC_CHECK_YEAR[DISTINCT_PC_YEAR] )
			                - MAX ( 'E10_PARA_PREVIOUS_YEAR_TBL'[Para_Previous_Year] )
			    )
			VAR START_POINT =
			    DATE ( YEAR (
			        CALCULATE (
			            [MIN_DATE_PC_CHECK],
			            ALLSELECTED ( E10_DIM_DATE ),
			            FILTER_PC_YEAR_TABLE
			        )
			    ), 01, 01 )
			VAR MAX_PC_CHECK_YEAR =
			    YEAR (
			        CALCULATE (
			            [MAX_DATE_PC_CHECK],
			            ALLSELECTED ( E10_DIM_DATE ),
			            FILTER_PC_YEAR_TABLE
			        )
			    )
			VAR PC_CHECK_YEAR =
			    CALCULATE (
			        MAX ( E10_PC_CHECKS[PC_CHECK_YEAR] ),
			        ALLSELECTED ( E10_DIM_DATE ),
			        FILTER_PC_YEAR_TABLE
			    )
			VAR END_POINT =
			    IF (
			        MAX_PC_CHECK_YEAR > YEAR ( START_POINT ),
			        CALCULATE (
			            [MAX_DATE_PC_CHECK],
			            ALLSELECTED ( E10_DIM_DATE ),
			            FILTER_PC_YEAR_TABLE
			        ),
			            IF(
			                YEAR(TODAY()) <= YEAR ( START_POINT ) && MONTH(TODAY())>= 10,
			                EOMONTH(DATE ( YEAR ( START_POINT ), 12, 31 ),3),
			                DATE ( YEAR ( START_POINT ), 12, 31 )
			            )
			    )
			RETURN
			    SWITCH (
			        TRUE (),
			        AND (
			            START_POINT <= MAX ( E10_DIM_DATE[FullDateAlternateKey] ),
			            MIN ( E10_DIM_DATE[FullDateAlternateKey] ) <= END_POINT
			        ), 1,
			        0
			    )
		formatString: 0
		displayFolder: PC_OVERTIME
		lineageTag: 1ec6ab90-359f-44c5-a563-e8e3666e13ba

