{
  "name": "Bosch's Visualization Template",
  "dataColors": [
    "#00884A",
    "#007BC0",
    "#FF5152",
    "#005691",
    "#007BC0",
    "#18837E",
    "#78BE20",
    "#00884A"
  ],
  "background": "#FFFFFF",
  "foreground": "#000000",
  "visualStyles": {
    "*": {
      "*": {
        "*": [
          {
            "fontSize": 10,
            "fontFamily": "Bosch Sans"
          }
        ],
        "visualTooltip": [
          {
            "titleFontColor": {
              "solid": {
                "color": "#D1D2D3"
              }
            },
            "valueFontColor": {
              "solid": {
                "color": "#FFFFFF"
              }
            },
            "background": {
              "solid": {
                "color": "#616263"
              }
            }
          }
        ],
        "visualHeaderTooltip": [
          {
            "titleFontColor": {
              "solid": {
                "color": "#D1D2D3"
              }
            },
            "background": {
              "solid": {
                "color": "#616263"
              }
            }
          }
        ]
      }
    },
    "lineStackedColumnComboChart": {
      "*": {}
    },
    "lineClusteredColumnComboChart": {
      "*": {}
    },
    "areaChart": {
      "*": {}
    },
    "lineChart": {
      "*": {}
    },
    "stackedAreaChart": {
      "*": {}
    }
  },
  "textClasses": {
    "callout": {
      "fontFace": "Bosch Sans"
    },
    "title": {
      "fontFace": "Bosch Sans"
    },
    "header": {
      "fontFace": "Bosch Sans"
    },
    "label": {
      "fontFace": "Bosch Sans"
    }
  }
}