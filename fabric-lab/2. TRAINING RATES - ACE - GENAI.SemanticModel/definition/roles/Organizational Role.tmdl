role 'Organizational Role'
	modelPermission: read

	tablePermission ListOfMembers = [userPrincipalName] == USERPRINCIPALNAME()

	tablePermission DIM_BU = ```
			SWITCH(TRUE(),
			        "ReportAdmin" in SELECTCOLUMNS(FILTER(ListOfMembers, ListOfMembers[userPrincipalName] = USERPRINCIPALNAME()), "Organizational Unit", ListOfMembers[OE Extract]), TRUE(), 
			        [ORG_AREA] in SELECTCOLUMNS(FILTER(ListOfMembers, ListOfMembers[userPrincipalName] = USERPRINCIPALNAME()), "Organizational Unit", ListOfMembers[OE Extract])
			        )
